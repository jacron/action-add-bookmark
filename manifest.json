{
  "name": "__MSG_addBookmark__",
  "description": "Bookmark may be duplicate",
  "version": "0.1",
  "permissions": [
    "bookmarks",
    "tabs",
    "activeTab"
  ],
  "background": {
    "scripts": ["background.js"]
  },
  "browser_action": {
    "default_icon": "images/star pink.png",
    "default_title": "Add bookmark",
    "default_popup": "popup.html"
  },
  "default_locale": "en",
  "manifest_version": 2
}
