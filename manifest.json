{
  "name": "__MSG_addBookmark__",
  "description": "Add a bookmark - may be a duplicate",
  "author": "Jan H Croonen",
  "version": "1.0",
  "permissions": [
    "bookmarks",
    "tabs",
    "activeTab",
    "storage"
  ],
  "background": {
    "page": "background.html",
    "persistent": false
  },
  "icons": {
    "16": "images/starpink16.png",
    "48": "images/starpink48.png",
    "128": "images/starpink128.png"
  },
  "browser_action": {
    "default_icon": "images/star pink.png",
    "default_title": "Add bookmark",
    "default_popup": "popup.html"
  },
  "default_locale": "en",
  "manifest_version": 2
}
